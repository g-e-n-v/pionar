{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.tsx",
    "src/preload/*.d.ts", 
    "src/renderer/main.tsx"  
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "~/*": [ "src/renderer/*" ],
      "#/*": [ "src/main/*" ],
      "~preload/*": [ "src/preload/*" ],
    }
  }
}
